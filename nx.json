{
  "npmScope": "productboard",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "eslint-config": {
      "tags": []
    },
    "tools": {
      "tags": []
    },
    "builder-test": {
      "tags": []
